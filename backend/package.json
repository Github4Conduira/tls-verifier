{
   "name": "@questbook/reclaim-backend",
   "version": "0.0.1",
   "description": "Reclaim backend",
   "main": "index.ts",
   "scripts": {
      "build": "npm exec tsc",
      "start": "node ./build",
      "start:tsc": "npm exec ts-node src",
      "test": "NODE_ENV=test jest",
      "lint": "eslint ./ --ext .ts,.jsx,.tsx",
      "lint:fix": "eslint ./ --fix --ext .ts,.jsx,.tsx",
      "generate:proto": "sh ./src/scripts/generate-proto.sh",
      "migrate": "npm exec ts-node -- ./node_modules/typeorm/cli.js migration:run -d src/repository/postgres/data-source.ts",
      "migrate:gen": "npm exec ts-node -- ./node_modules/typeorm/cli.js migration:generate -d src/repository/postgres/data-source.ts src/repository/postgres/migrations/$NAME"
   },
   "author": "Alex E",
   "license": "ISC",
   "dependencies": {
      "@questbook/reclaim-node": "file:../node",
      "@reclaimprotocol/crypto-sdk": "file:../crypto-sdk",
      "base64url": "^3.0.1",
      "dotenv": "^16.0.3",
      "ethers": "^5.7.2",
      "firebase-admin": "^11.5.0",
      "nice-grpc": "^2.1.0",
      "pg": "^8.9.0",
      "pino": "^8.9.0",
      "protobufjs": "^7.2.2",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.3.12",
      "typeorm-naming-strategies": "^4.1.0"
   },
   "devDependencies": {
      "@adiwajshing/eslint-config": "github:adiwajshing/eslint-config",
      "@typechain/ethers-v5": "^10.2.0",
      "@types/chance": "^1.1.3",
      "@types/jest": "^29.4.0",
      "@types/node": "^18.13.0",
      "@types/pino": "^7.0.5",
      "chance": "^1.1.9",
      "eslint": "^8.33.0",
      "jest": "^29.4.2",
      "ts-jest": "^29.0.5",
      "ts-node": "^10.9.1",
      "ts-proto": "^1.139.0",
      "typechain": "^8.1.1",
      "typescript": "^4.9.5"
   }
}
