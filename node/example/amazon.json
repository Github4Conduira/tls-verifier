{
  "name": "amazon-order-history",
  "params": {
    "url": "https://www.amazon.com/gp/your-account/order-history/ref=ppx_yo_dt_b_search?opt=ab&search={{AMAZON_PRODUCT}}",
    "method": "GET",
    "responseSelection": [
      {
        "xPath": "//div[@class='a-fixed-left-grid a-spacing-double-large']/div[@class='a-fixed-left-grid-inner']/div[@class='a-fixed-left-grid-col a-col-right']/div[@class='a-row']/a",
        "responseMatch": "<a class=\"a-link-normal\"[a-z\\s%0-9&(),\"\\/=\\?&;_]+>[\\n\\s\\w\\-%&(),\"\\/=\\?&;_]+<\\/a>"
      }
    ]
  },
  "secretParams": {
    "cookieStr": "{{AMAZON_COOKIE}}"
  }
}
